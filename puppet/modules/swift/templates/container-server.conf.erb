[DEFAULT]
bind_ip = 0.0.0.0
bind_port = 6011
mount_check = false
user = root
devices = <%= swift_devices %>
workers = 2
swift_dir = /etc/swift
log_facility = LOG_LOCAL2
log_level = DEBUG

[pipeline:main]
pipeline = container-server

[app:container-server]
use = egg:swift#container

[container-replicator]

[container-updater]

[container-auditor]

[container-sync]
