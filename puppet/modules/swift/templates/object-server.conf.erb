[DEFAULT]
bind_ip = 0.0.0.0
bind_port = 6010
user = root
devices = <%= swift_devices %>
mount_check = false
swift_dir = /etc/swift
workers = 2

log_facility = LOG_LOCAL3
log_level = DEBUG

[pipeline:main]
pipeline = object-server

[app:object-server]
use = egg:swift#object

[object-replicator]

[object-updater]

[object-auditor]
